{
  "configurations": {

    "default": {
      "system": {
        "mem_mode": "timing",  
        "clock_domain": {
          "clock": "4GHz",     
          "voltage_domain": {
            "voltage": "2.0V"
          }
        }
      },
      "cpu": {
        "type": "O3CPU",  
        "num_cores": 1,             
        "branch_prediction": {
          "enabled": false          
        }
      },
      "memory": {
        "override_gem5_data": false,
        "dram": {
          "type": "DDR3_1600_8x8",
          "ranks_per_channel": 1,   
          "channels": 1,            
          "size": "512MB"           
        }
      },
      "cache_hierarchy": {
        "l1i": {
          "size": "16kB",           
          "assoc": 1
        },
        "l1d": {
          "size": "16kB",
          "assoc": 1
        },
        "l2": {
          "size": "256kB",
          "assoc": 4,
          "tag_latency": 10,        
          "data_latency": 10,
          "response_latency": 10,
          "mshrs": 10,
          "tgts_per_mshr": 8
        }
      },
      "process": {
        "binary": "/simulator_configs/gem5/ARM/iforest_embedded_arm"
      }
    }
  }
}